@{
    ViewData["Title"] = "Welcome to";
}

    <style>
        body {
            font-family: Arial, sans-serif;
            margin-bottom:0px;
        }

        .hero {
            background-color: #f9f9f9;
            padding: 100px 0;
            text-align: center;
        }

        .hero h1 {
            font-size: 48px;
            font-weight: bold;
            margin-bottom: 20px;
        }

        .hero p {
            font-size: 20px;
            color: #333;
        }

        .features {
            background-color: #fff;
            padding: 80px 0;
            text-align: center;
        }

        .features h2 {
            font-size: 36px;
            font-weight: bold;
            margin-bottom: 40px;
        }

        .features .feature-item {
            margin-bottom: 40px;
        }

        .features .feature-icon {
            font-size: 48px;
            color: #f9f9f9;
            margin-bottom: 20px;
        }

        .footer {
            background-color: #f9f9f9;
            color: #fff;
            padding: 20px 0;
            text-align: center;
        }

        .footer p {
            margin: 0;
        }
        .welcome{
            background-image:url('../Backgrounds/home.jpeg');
            background-repeat:no-repeat;
            background-size:cover;
            color:white;
            padding:200px 0;
        }
        .welcome p {
             color:white;
        }
    </style>

